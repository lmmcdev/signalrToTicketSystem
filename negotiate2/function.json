{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["post"],
      "route": "negotiate"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    },
    {
      "type": "signalRConnectionInfo",
      "name": "signalRConnectionInfo",
      "hubName": "ticketshubchannels",
      "connection": "AzureSignalRConnectionString",
      "userId": "{body.userId}"
    }
  ]
}
